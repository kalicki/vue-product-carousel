{
  "name": "vue-product-carousel",
  "version": "0.2.0",
  "description": "Simple slider on Vue.js with hot image replacement, Zoom and Swipe mode",
  "author": "Max Lyashuk <m_lyashuk@ukr.net>",
  "private": false,
  "main": "dist/carousel.min.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.config.js",
    "build:dist": "cross-env NODE_ENV=production webpack --config build/webpack.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/probil/vue-product-carousel.git"
  },
  "devDependencies": {
    "vue": "^2.0.1",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.13.2",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.4",
    "vue-loader": "^9.4.0",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/probil/vue-product-carousel/blob/master/LICENSE"
    }
  ]
}
